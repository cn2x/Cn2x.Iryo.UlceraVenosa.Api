      "LargeObjectHeapCompactionMode": "CompactOnce"
    },
    "ThreadPool": {
      "MinThreads": 4,
      "MaxThreads": 8
    }
  },
  "HealthChecks": {
    "PostgreSQL": {
      "Timeout": "00:00:05"
    },
    "Memory": {
      "MaximumMegabytesAllocated": 1024
    },
    "Disk": {
      "MinimumFreeSpaceGB": 1
    },
    "Application": {
      "MaximumMemoryMB": 500
    }
  }
